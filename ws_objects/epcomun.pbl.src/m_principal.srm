  If f_respaldabd('069', True) <= 0 Then
   ROLLBACK USING SQLCa;
   Return 
  End If 
 End If 
Else
 MessageBox('ATENCION:', ' Solo un usuario con nivel de administrador puede respaldar la base de datos. GRACIAS', Information!)
End If
end event

