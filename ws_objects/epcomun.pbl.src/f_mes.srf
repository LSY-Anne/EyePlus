global type f_mes from function_object
end type

forward prototypes
global function string f_mes (integer ai_mes)
end prototypes

global function string f_mes (integer ai_mes);String ls_mes

Choose Case ai_mes
	Case 1
		ls_mes = "Enero"
	Case 2
		ls_mes = "Febrero"
	Case 3
		ls_mes = "Marzo"
	Case 4
		ls_mes = "Abril"
	Case 5
		ls_mes = "Mayo"
	Case 6
		ls_mes = "Junio"
	Case 7
		ls_mes = "Julio"		
	Case 8
		ls_mes = "Agosto"
	Case 9
		ls_mes = "Septiembre"
	Case 10
		ls_mes = "Octubre"
	Case 11
		ls_mes = "Noviembre"
	Case 12
		ls_mes = "Diciembre"
End Choose

Return ls_mes 


end function

